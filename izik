Ȏ[ȡ]);Ë();ȣ=(int)Math.Floor((Ǡ>20?20:Ǡ)/maxCurrentMs);if(Ƞ){Ƞ=false;}else if(ȡ>=19){ȡ=0;ȉ=new HashSet<string>(Ȥ);Ȥ.Clear(
);if(ʁ>0)ʁ--;if(ȉ.Count==0)Ŋ=null;}else{ȡ++;}}catch(NullReferenceException e){ʁ++;ɬ=true;ɺ=false;ʀ=e.ToString();ǁ(
"Execution of script step aborted:\n"+Ȏ[ȡ]+" (ID: "+ȡ+")\n\nCached block not available..");}catch(Exception e){ʁ++;ɬ=true;ɺ=false;ʀ=e.ToString();ǁ(
"Critical error in script step:\n"+Ȏ[ȡ]+" (ID: "+ȡ+")\n\n"+e);}}bool ȍ(string Ȍ){if(Ȍ.Contains("pauseThisPB")){Echo("Script execution paused!\n");var ȋ=Ȍ.
Split(';');if(ȋ.Length==3){Echo("Found:");Echo("'"+ȋ[1]+"'");Echo("on grid:");Echo("'"+ȋ[2]+"'");Echo(
"also running the script.\n");Echo("Type container protection: "+(protectTypeContainers?"ON":"OFF")+"\n");Echo(
"Everything else is managed by the other script.");}return true;}bool Ȋ=true;bool ȇ=true;bool ȹ=false;if(Ȍ!="reset"&&Ȍ!="msg"){if(!Ȍ.Contains(" on")&&!Ȍ.Contains(" off")
&&!Ȍ.Contains(" toggle"))return false;if(Ȍ.Contains(" off"))ȇ=false;if(Ȍ.Contains(" toggle"))ȹ=true;}if(Ȍ=="reset"){ǀ();
